    "version": "0.2.0",
    "configurations":
    [
      {
        "name": "(gdb) runoff",
        "type": "cppdbg",
        "request": "launch",
        "program": "C:\\EmbeddedDevelopment\\Tutorial\\CS50Course\\runoff\\runoff.exe",
        "args": ["german", "idoate", "gil"],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": true,
        "MIMode": "gdb",
